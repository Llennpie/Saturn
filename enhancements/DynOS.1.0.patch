+            play_sound(SOUND_DYNOS_SAVED, gDefaultSoundArgs);
+        }
+#ifdef BETTERCAMERA
+        newcam_init_settings();
+#endif
+        controller_reconfigure();
+        configfile_save(configfile_name());
+        DynOS_Opt_SaveConfig(sDynosMenu);
+        sCurrentMenu = NULL;
