+            case DOPT_CHOICELEVEL: *_Opt->mChoice.mIndex = aValue; break;
+            case DOPT_CHOICEAREA:  *_Opt->mChoice.mIndex = aValue; break;
+            case DOPT_CHOICESTAR:  *_Opt->mChoice.mIndex = aValue; break;
+#ifndef DYNOS_COOP
