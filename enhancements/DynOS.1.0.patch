+template <typename T>
+T* New(u64 aCount = 1llu) {
+    T *_Ptr = (T *) calloc(aCount, sizeof(T));
+    for (u64 i = 0; i != aCount; ++i) {
+        new (_Ptr + i) T(); // Calls the constructor of type T at address (_Ptr + i)
+    }
+    return _Ptr;
+}
+
+template <typename T>
+void Delete(T *& aPtr) {
+    if (aPtr) aPtr->~T();
+    free(aPtr);
+    aPtr = NULL;
+}
